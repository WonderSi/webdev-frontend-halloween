{
  "name": "web-frontend-semestr5-halloween",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p sass:dev dev:only",
    "sass:dev": "sass --watch src/styles/main.scss:src/style.css",
    "dev:only": "vite",
    "sass:prod": "sass --no-source-map --style compressed src/styles/main.scss:dist/style.css",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.93.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  }
}
